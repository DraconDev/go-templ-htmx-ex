{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": [],
            "env": {
                "DEBUG": "debug",
                "AUTH_SERVICE_URL": "http://localhost:8080",
                "PORT": "8081",
                "REDIRECT_URL": "http://localhost:8081"
            },
            "console": "integratedTerminal",
            "showLog": true,
            "logOutput": "debugger",
            "subProcess": true,
            "integratedAdapter": {
                "dll": "",
                "dwp": ""
            }
        }
    ]
}